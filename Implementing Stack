#include <iostream>
using namespace std;

class Stack {
    int arr[10];
    int top;

public:
    Stack() {
        top = -1;
    }

    void push(int a) {
        if (!isFull()) {
            top++;
            arr[top] = a;
        } else {
            cout << "Stack is Full" << endl;
        }
    }

    int pop() {
        if (isEmpty()) {
            cout << "Stack is Empty" << endl;
            return -1;
        } else {
            int temp = arr[top];
            top--;
            return temp;
        }
    }

    bool isEmpty() {
        return top == -1;
    }

    bool isFull() {
        return top == (10 - 1);
    }

    int peak() {
        if (isEmpty()) {
            cout << "Stack is Empty" << endl;
            return -1;
        }
        return arr[top];
    }
};


int main() {
    Stack s;
    s.push(10);
    s.push(5);
    s.push(23);
    s.push(4);
    s.push(1);

    int a = s.pop();
    cout << "Popped: " << a << endl;

    s.push(20);
    s.push(10);
    int b= s.pop();

    cout << "Top element: " << s.peak() << endl;

    return 0;
}
